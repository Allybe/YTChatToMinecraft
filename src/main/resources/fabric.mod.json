{
	"schemaVersion": 1,
	"id": "youtube-chat",
	"version": "${version}",
	"name": "YouTube Chat",
	"description": "A Minecraft mod that brings any YouTube Stream chat into Minecraft ",
	"authors": [
		"Allison"
	],
	"contact": {
		"sources": "https://github.com/Allybe/YTChatToMinecraft"
	},
	"license": "CC0-1.0",
	"icon": "assets/youtube-chat/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			{
				"value": "ally.ytchat.YouTubeChatClient",
				"adapter": "kotlin"
			}
		]
	},
	"depends": {
		"fabricloader": ">=0.15.0",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.23"
	}
}